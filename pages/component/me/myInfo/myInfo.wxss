/* pages/me/myInfo/myInfo.wxss */
page {
  background-color: #F0F0F0;
}
.center {
  width: 100%;
  height: 660rpx;
  background-color: #FFFFFF;
  margin-top: 14rpx;
  box-sizing: border-box;
}
.myInfo-list {
  width: 668rpx;
  height: 64rpx;
  margin-left: 44rpx;
  border-bottom: 2rpx solid rgba(198, 198, 198, 0.253);
  display: flex;
  justify-content: space-between;
}
.myInfo-list:nth-child(4),
.myInfo-list:nth-child(7){
  border-top: 2rpx solid rgba(198, 198, 198, 0.253);
}
.myInfo-list .head {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  margin-top: 4rpx;
}
.myInfo-list:last-child {
  border-bottom: 0;
}
.myInfo-list .myInfo-txt {
  font-size: 24rpx;
  line-height: 64rpx;
}
.myInfo-list .myInfo-txt image {
  width: 12rpx;
  height: 18rpx;
  margin-left: 8rpx;
  transform: translateY(2rpx);
}